             xmlns:d="http://xamarin.com/schemas/2014/forms/design"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             xmlns:maps="clr-namespace:DnDApp.Maps"
             mc:Ignorable="d"
             x:Class="DnDApp.Maps.MapEditPage">
