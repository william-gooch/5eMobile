        <Grid>
            <Grid.RowDefinitions>
                <RowDefinition Height="*" />
            </Grid.RowDefinitions>
            <Grid.ColumnDefinitions>
                <ColumnDefinition Width="*" />
            </Grid.ColumnDefinitions>
            <maps:MapView Grid.Row="0" Grid.Column="0" />
        </Grid>
    </ContentPage.Content>
</ContentPage>